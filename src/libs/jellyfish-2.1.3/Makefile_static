all: bin/jellyfish_exe

bin/jellyfish_exe:
	g++ -std=c++0x -g -O3 -std=c++0x -o bin/jellyfish sub_commands/jellyfish.o sub_commands/count_main.o sub_commands/info_main.o sub_commands/dump_main.o sub_commands/histo_main.o sub_commands/stats_main.o sub_commands/merge_main.o sub_commands/bc_main.o sub_commands/query_main.o sub_commands/cite_main.o jellyfish/merge_files.o  -lpthread ./.libs/libjellyfish-2.0.so -Wl,-rpath -Wl,.libs
	ln -s jellyfish bin/jellyfish_exe
	cp -ar .libs/* bin/.libs/

